mixin page-advertising()
  .page-services
    .container
      .page-services__grid(data-tabs="parent")
        .page-services__navigation(data-tabs="controls")
          +service-nav(advertisingNavigation)
        .page-services__content
          div(data-tabs="content")
            .service-content.page-content(data-tabs="element")
              h2.service-content__title Радиостанции
              .page-content__text
                p Что-то про рекламу на радиостанциях и т.д.
              .page-content__image.page-content__image--main
                +picture("content/advertising", "advertising-radio", "jpg", 695, 420)
              +service-item(nasheRadioData).service-item--radio
              +service-item(recordRadioData).service-item--radio
              +service-item(vanyaRadioData).service-item--radio
              +service-item(mariaRadioData).service-item--radio
              +service-item(rockRadioData).service-item--radio
            .service-content.page-content(data-tabs="element")
              h2.service-content__title Социальные сети
              .page-content__text
                p Что-то про социальные сети их крутость и т.д.
              .page-content__image.page-content__image--main
                +picture("content/advertising", "advertising-social", "jpg", 695, 420)
              +service-item(socialArzLiveData).service-item--socials
              +service-item(socialArzOverheardData).service-item--socials
              +service-item(socialAutoSaleData).service-item--socials
              +service-item(socialArzRacingData).service-item--socials
              +service-item(socialArzKringeData).service-item--socials
              +service-item(socialArzJunkData).service-item--socials
              +service-item(socialArzTopData).service-item--socials
              +service-item(socialArzVTopData).service-item--socials
            .service-content.page-content(data-tabs="element")
